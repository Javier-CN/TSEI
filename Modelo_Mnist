      "source": [
        "def load_samples():\n",
        "\n",
        "    f = gzip.open('mnist.pkl.gz', 'rb')\n",
        "    \n",
        "    learn_samples, val_samples, test_samples = pickle.load(f, encoding=\"latin1\")\n",
        "    \n",
        "    f.close()\n",
        "    \n",
        "    return (learn_samples, val_samples, test_samples)\n",
        "\n",
        "learn_samples, val_samples, test_samples = load_samples()"
      ]
    },
